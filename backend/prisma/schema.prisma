// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Car {
  id  Int @id @default(autoincrement())
  nome String
  marca String
  modelo String
  valor Float
  foto String
}

model User {
  id Int @id @default(autoincrement())
  email String
  hashPassword String
}
